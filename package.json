{
  "name": "github-contributors-map",
  "version": "1.0.0-alpha.1",
  "description": "A world map that shows contributors of a given GitHub repository",
  "private": true,
  "scripts": {
    "clean": "rm -Rf pnpm-lock.yaml node_modules",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --base /github-contributors-map/",
    "serve": "vite",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "keywords": [
    "github",
    "map"
  ],
  "author": {
    "name": "Christopher Quadflieg",
    "email": "chrissi92@hotmail.de",
    "url": "https://github.com/Shinigami92"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shinigami92/github-contributors-map.git"
  },
  "bugs": "https://github.com/Shinigami92/github-contributors-map/issues",
  "license": "MIT",
  "dependencies": {
    "@octokit/graphql": "~4.8.0",
    "three": "~0.133.1",
    "vue": "~3.2.20"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "~3.2.0",
    "@types/node": "~22.7.9",
    "@types/three": "~0.133.1",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "@vitejs/plugin-vue": "~5.1.4",
    "@vue/compiler-sfc": "~3.2.20",
    "@vue/eslint-config-prettier": "~10.1.0",
    "@vue/eslint-config-typescript": "~14.1.3",
    "@windicss/plugin-icons": "~1.0.2",
    "csv-parse": "~5.5.6",
    "eslint": "~9.13.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-define-config": "~2.1.0",
    "eslint-import-resolver-typescript": "~3.6.3",
    "eslint-plugin-import": "~2.25.2",
    "eslint-plugin-prettier": "~5.2.1",
    "eslint-plugin-vue": "~9.29.1",
    "eslint-plugin-vue-pug-sfc": "~1.0.0-alpha.17",
    "esno": "~4.8.0",
    "prettier": "~3.3.3",
    "prettier-plugin-organize-imports": "~4.1.0",
    "sass": "~1.43.3",
    "typescript": "~5.6.3",
    "vite": "~5.4.10",
    "vite-plugin-windicss": "~1.4.12",
    "vue-tsc": "~2.1.6"
  }
}
