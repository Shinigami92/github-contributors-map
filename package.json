{
  "name": "github-contributors-map",
  "version": "1.0.0-alpha.1",
  "description": "A world map that shows contributors of a given GitHub repository",
  "private": true,
  "scripts": {
    "clean": "rm -Rf pnpm-lock.yaml node_modules",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --base /github-contributors-map/",
    "serve": "vite",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "keywords": [
    "github",
    "map"
  ],
  "author": {
    "name": "Christopher Quadflieg",
    "email": "chrissi92@hotmail.de",
    "url": "https://github.com/Shinigami92"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shinigami92/github-contributors-map.git"
  },
  "bugs": "https://github.com/Shinigami92/github-contributors-map/issues",
  "license": "MIT",
  "dependencies": {
    "@octokit/graphql": "~4.8.0",
    "three": "~0.169.0",
    "vue": "~3.5.0"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "~1.17.0",
    "@types/node": "~16.11.4",
    "@types/three": "~0.133.1",
    "@typescript-eslint/eslint-plugin": "5.1.1-alpha.8",
    "@typescript-eslint/parser": "5.1.1-alpha.8",
    "@vitejs/plugin-vue": "~1.9.3",
    "@vue/compiler-sfc": "~3.2.20",
    "@vue/eslint-config-prettier": "~6.0.0",
    "@vue/eslint-config-typescript": "~8.0.0",
    "@windicss/plugin-icons": "~1.0.2",
    "csv-parse": "~4.16.3",
    "eslint": "~8.1.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-define-config": "~1.1.2",
    "eslint-import-resolver-typescript": "~2.5.0",
    "eslint-plugin-import": "~2.25.2",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-vue": "~7.20.0",
    "eslint-plugin-vue-pug-sfc": "~1.0.0-alpha.17",
    "esno": "~0.10.1",
    "prettier": "~2.4.1",
    "prettier-plugin-organize-imports": "~2.3.4",
    "sass": "~1.43.3",
    "typescript": "~4.4.4",
    "vite": "~2.6.10",
    "vite-plugin-windicss": "~1.4.12",
    "vue-tsc": "~0.28.8"
  }
}
